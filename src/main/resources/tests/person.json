{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "nome",
    "idade"
  ],
  "properties": {
    "nome": {
      "$id": "#/properties/nome",
      "type": "string",
      "title": "The Nome Schema",
      "default": "",
      "examples": [
        "Eduardo"
      ],
      "pattern": "^(.*)$"
    },
    "idade": {
      "$id": "#/properties/idade",
      "type": "integer",
      "title": "The Idade Schema",
      "default": 0,
      "examples": [
      ]
    }
  }
}